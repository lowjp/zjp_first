
.content {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.logo {
    height: 200rpx;
    width: 200rpx;
    margin-top: 200rpx;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 50rpx;
}
.text-area {
    display: flex;
    justify-content: center;
}
.title {
    font-size: 36rpx;
    color: #8f8f94;
}
.status_bar {
    height: var(--status-bar-height);
    width: 100%;
}
.hover_button {
    width: 70px;
    height: 70px;
}
.importantandurgent {
    background: #e43d33;
}
.importantandnurgent {
    background: #f3a73f;
}
.nimportantandurgent {
    background: #2979ff;
}
.free {
    background: #8f939c;
}
